{
  "name": "mjc-nuxt-starter",
  "version": "1.0.0",
  "description": "Nuxt.js starter for new Vue projects",
  "author": "Matthew Jason Craig",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
